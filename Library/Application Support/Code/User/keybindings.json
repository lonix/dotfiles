[{
  "key": "ctrl+alt+[BracketLeft]",
  "command": "git.commit",
  "when": "editorFocus"
},
{
  "key": "cmd+k",
  "command": "workbench.action.terminal.clear",
  "when": "terminalFocus"
},{
  "key": "ctrl+t",
  "command": "extension.transpose",
  "when": "editorFocus"
},{
  "key": "cmd+l",
  "command": "extension.expandToLine",
  "when": "editorFocus"
},{
  "key": "shift+cmd+l",
  "command": "extension.splitIntoLines",
  "when": "editorFocus"
},{
  "key": "ctrl+alt+p",
  "command": "git.push",
  "when": "editorFocus"
},{
  "key": "cmd+j",
  "command": "extension.joinLines",
  "when": "editorFocus"
},
]
